<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>admin</servlet-name>
		<display-name>admin</display-name>
		<description></description>
		<servlet-class>com.AdminServlets.AdminServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>addTrain</servlet-name>
		<display-name>addTrain</display-name>
		<description></description>
		<servlet-class>com.AdminServlets.AddTrainServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>usersList</servlet-name>
		<display-name>usersList</display-name>
		<description></description>
		<servlet-class>com.AdminServlets.ViewAllUsers</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addTrain</servlet-name>
		<url-pattern>/addTrain</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>usersList</servlet-name>
		<url-pattern>/usersList</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Welcome.html</welcome-file>
		<welcome-file>AViewAllUsers.jsp</welcome-file>
	</welcome-file-list>
</web-app>
